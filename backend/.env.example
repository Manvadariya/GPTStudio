# AI Provider Config
AZURE_AI_ENDPOINT="your_azure_ai_endpoint_here"
AZURE_AI_API_KEY="your_azure_ai_api_key_here"
EMBEDDING_MODEL_NAME="openai/text-embedding-3-small"
CHAT_MODEL_NAME="openai/gpt-4o-mini"

# Database for Application Data (Users, Projects, etc.)
APP_MONGO_URI="mongodb+srv://username:password@cluster.mongodb.net/ai_platform_db?retryWrites=true&w=majority"

# Database for RAG Vector Chunks
RAG_MONGO_URI="mongodb+srv://username:password@cluster.mongodb.net/rag_db?retryWrites=true&w=majority"

# Secret for signing JSON Web Tokens
JWT_SECRET="your_jwt_secret_here"

# Server Port
PORT=3001
